{
  "kind": "collectionType",
  "collectionName": "videos",
  "info": {
    "singularName": "video",
    "pluralName": "videos",
    "displayName": "videos",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comments.comments"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "videos"
    },
    "waitingroom": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "views": {
      "type": "integer",
      "required": true,
      "default": 1
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "youtube": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "grades": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::grade.grade",
      "mappedBy": "video"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tags.tags",
      "mappedBy": "video"
    }
  }
}
